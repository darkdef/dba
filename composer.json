{
    "name": "yiisoft/dbal",
    "type": "library",
    "description": "Yii DataBase Access Library",
    "keywords": [
        "yii",
        "database"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/dbal",
        "issues": "https://github.com/yiisoft/dbal/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "roave/infection-static-analysis-plugin": "^1.5",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Dbal\\": "src",
            "Yiisoft\\DbalMysql\\": "dbms\\Mysql"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Dbal\\Tests\\": "tests",
            "Yiisoft\\DbalMysql\\Tests\\": "tests\\dbms\\Mysql"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
